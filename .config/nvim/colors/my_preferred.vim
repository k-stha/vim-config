highlight clear
syntax reset

let g:colors_name="my_preferred"

highlight Comment ctermfg=8 guifg=#808080
highlight Constant ctermfg=white guifg=white
highlight ExtraWhitespace ctermbg=green guibg=green
highlight MsgArea ctermfg=white guifg=white
highlight Normal ctermfg=grey ctermbg=black guifg=grey guibg=black
highlight Number ctermfg=white guifg=white
highlight Pmenu ctermfg=white ctermbg=DarkMagenta guifg=black guibg=Magenta
highlight PmenuSel ctermfg=white ctermbg=black guifg=white guibg=black
highlight SpellBad ctermfg=white ctermbg=red guifg=white guibg=red
highlight SpellLocal ctermfg=black guifg=black
highlight Statement ctermfg=yellow guifg=yellow
highlight StatusLine ctermfg=white ctermbg=black guifg=white guibg=black
highlight String ctermfg=white guifg=white
highlight TaskDeadline ctermfg=red guifg=red
highlight TaskDone ctermfg=green guifg=green
highlight Visual ctermfg=black ctermbg=white guifg=black guibg=white
